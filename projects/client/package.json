{
  "name": "client",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "dev:contrib": "IS_CONTRIB=true vite dev",
    "build": "vite build",
    "build:preview": "IS_PREVIEW=true vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:unit": "vitest",
    "test:cov": "vitest --coverage",
    "test:e2e": "PW_DISABLE_TS_ESM=true playwright test",
    "i18n:resolve": "deno run --allow-read --allow-write .scripts/resolve-i18n.ts",
    "i18n:traktify": "deno run --allow-read --allow-write --allow-net --allow-env .scripts/traktify-i18n.ts",
    "i18n:delete": "deno run --allow-read --allow-write --allow-env .scripts/delete-i18n.ts",
    "cloudflare:cleanse": "deno run --allow-env --allow-net .scripts/cleanse-cloudflare.ts",
    "insecurity:generate": "deno run .scripts/generate-insecurity.ts"
  },
  "devDependencies": {
    "@google/generative-ai": "^0.21.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "4.9.0",
    "@sveltejs/enhanced-img": "^0.4.4",
    "@sveltejs/kit": "^2.15.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^9.6.1",
    "@vitest/coverage-istanbul": "3.0.0-beta.4",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "sass-embedded": "^1.83.1",
    "svelte": "^5.17.3",
    "svelte-check": "^4.1.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vitest": "3.0.0-beta.4"
  },
  "dependencies": {
    "@inlang/paraglide-sveltekit": "^0.15.5",
    "@tanstack/svelte-query": "^5.62.16",
    "@tanstack/svelte-query-devtools": "^5.62.16",
    "@tanstack/svelte-query-persist-client": "^5.64.2",
    "@ts-rest/core": "^3.51.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.1.0",
    "idb-keyval": "^6.2.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  }
}
